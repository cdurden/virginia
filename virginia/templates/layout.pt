<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      metal:define-macro="layout">
<head>
    <meta charset="UTF-8">
    <title>${title}</title>
    <link rel="stylesheet" href="${request.static_url('virginia:static/css/main.css')}"/>
    <script tal:condition="context.use_mathjax" src="${request.static_url('virginia:static/js/MathJax/MathJax.js')}?config=TeX-AMS-MML_HTMLorMML" type="text/javascript"></script>
<!--
    <script tal:condition="context.use_mathjax" src="${request.static_url('virginia:static/js/MathJax/MathJax.js')}" type="text/javascript"></script>
    <script tal:condition="context.use_mathjax" type="text/javascript"> MathJax.Hub.Config({ "tex2jax": { inlineMath: [ [ '$', '$' ] ] } }); </script>-->
    <tal:block repeat="js python: context.js">
      <script type="text/javascript">${structure: js}</script>
    </tal:block>
    <tal:block repeat="css python: context.css">
      <style>${structure: css}</style>
    </tal:block>
    <more metal:define-slot="head-more"></more>
</head>
<body>
<header>
    <h1><img src="/images/jma_crest.png" alt="JMA crest" align="middle" style="height:144px;"/> JMA 7th grade math <img src="images/pi_pencils.png" align="middle" style="height:144px;"/> </h1> 
</header>

<div id="main">
    <tal:block metal:define-slot="title">
    </tal:block>
    <div id="nav"> 
    <tal:block metal:define-slot="nav">
      ${structure: context.nav}
    </tal:block>
    </div>
    <div id="content">
      <tal:block metal:define-slot="content">
        ${structure: content}
      </tal:block>
    </div>
</div>
</body>
</html>
